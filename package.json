    "email": "scommisso@godaddy.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/godaddy/node-priam.git"
  },
  "dependencies": {
    "helenus": "~0.6.8",
    "node-cassandra-cql": "~0.4.4",
    "lodash": "~2.4.1",
    "async": "~0.7.0",
    "q": "^1.0.1"
  },
  "devDependencies": {
    "mocha": "^1.18.2",
    "chai": "^1.9.1",
    "sinon": "^1.9.1",
    "istanbul": "^0.2.4",
    "mocha-istanbul": "^0.2.0",
    "jshint": "^2.5.0",
    "winston": "^0.7.3",
    "coveralls": "^2.10.0",
